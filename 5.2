#include <stdio.h>
#include <iostream>
#include <cmath> 
#include <math.h>
#include <stack>

using namespace std;
int main()
{
    int N;
    cout << "Enter your number   ";
    cin >> N;
    int a[10000];

    for (int i = 0; i < N; i++) {
        a[i] = i;
    }
    a[1] = 0;
    for (int s = 2; s < N; s++) {
        if (a[s] != 0) {
            for (int j = s * 2; j < N; j += s) {
                a[j] = 0;
            }
        }
    }
    for (int i = 0; i < N; i++) {
        if (a[i] != 0) {
            cout << a[i] << endl;
        }
    }
}
