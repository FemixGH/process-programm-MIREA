#include <iostream>
#include <cmath> 
using namespace std;
int main()
{
    float a, b, c, Discreminant;
    float x1, x2;
    cout << "Enter a: "<<endl;
    cin >> a;
    cout << "Enter b: "<<endl;
    cin >> b;
    cout << "Enter c: "<<endl;
    cin >> c;
    if (a == 0) {
        cout << "The answer is unattainable" << endl;
        exit(0);
    }
    if (a + b + c == 0) {
        x1 = 1;
        x2 = c / a;
        cout <<"x1 = "  << x1<<endl;
        cout << "x2 = "<< x2 << endl;
        exit(0);
    }

    Discreminant = (b * b) - (4 * a * c);
    if (Discreminant < 0) {
        cout << "The answer is unattainable" << endl;
        exit(0);
    }
    if (Discreminant == 0) {
        x1 = (b * (-1)) / 2 * a;
        cout << "x = " << x1 << endl;
    }
    x1 = ((b * (-1)) + sqrt(Discreminant)) / 2 * a;
    x2 = ((b * (-1)) - sqrt(Discreminant)) / 2 * a;
    cout << "x1 = " << x1 << endl;
    cout << "x2 = " << x2 << endl;
}
