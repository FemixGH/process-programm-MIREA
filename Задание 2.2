#include <iostream>
#include <cmath> 
#include <math.h>

using namespace std;
int main()
{
	float x, a, result; 
	cout << "enter x: " << endl;
	cin >> x;
	cout << "enter a: " << endl;
	cin >> a;

	if (fabs(x) < 1) {
		result = a * log(x);
		cout << "Your result is " << result;
		exit(0);
	}
	if (fabs(x) >= 1) {
		if ((a - (x * x)) <= 0) {
			cout << "Error" << endl;
		}
		result = sqrt(a - ((x * x)));
		cout << "Your result is " << result;
		exit(0);
	}
}
